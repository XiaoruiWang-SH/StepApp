<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- 设置圆形按钮的颜色 -->
    <solid android:color="#A8E6CF"/> <!-- 浅绿色背景 -->

    <!-- 设置按钮边框 -->
    <stroke
        android:width="12dp"
        android:color="#cce0bc"/> <!-- 浅浅绿色边框 -->

    <!-- 设置按钮的大小 -->
    <size
        android:width="150dp"
        android:height="150dp"/>
</shape>
